{
	"include": ["**/*.ts", "**/*.tsx"],
	"compilerOptions": {
		"lib": ["ES2019", "dom"],
		"isolatedModules": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"target": "ES2019",
		"strict": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true
	}
}
